extends ../main

block variables
    - var title = 'Maxirolls'
    - var basketHeader = true

block content
    main.order
        .container
            h1.order__title Заказ на доставку
            .order__grid
                form(action='info.html' method='POST').order__right
                    .order__contacts
                        .ocontacts
